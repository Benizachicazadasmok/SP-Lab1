import java.util.ArrayList;
import java.util.List;

public class Element {

	List<Paragraph> p = new ArrayList<Paragraph>();
	List<Image> i = new ArrayList<Image>();
	List<Table> t = new ArrayList<Table>();

	public Element(List<Paragraph> p, List<Image> i, List<Table> t) {
		this.p = p;
		this.i = i;
		this.t = t;
	}

	public void print() {

		System.out.println(this.p[i] + this.i + this.t);
	}
}
