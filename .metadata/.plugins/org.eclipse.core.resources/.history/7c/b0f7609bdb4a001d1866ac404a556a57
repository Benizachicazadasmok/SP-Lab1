import java.util.ArrayList;
import java.util.List;

public class Element {

	List<Paragraph> p = new ArrayList<Paragraph>();
	List<Image> i = new ArrayList<Image>();
	List<Table> t = new ArrayList<Table>();
 
	public Element(List<Paragraph> p) {
		super();
		this.p = p;
	}
	

	public Element(List<Image> i) {
		super();
		this.i = i;
	}


	public Element(List<Table> t) {
		super();
		this.t = t;
	}


	public void print() {

		System.out.println(this.p + this.i + this.t);
	}
}
