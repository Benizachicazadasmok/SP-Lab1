import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;

public class SubChapter implements Element {

	String name;

	List<Element> e = new ArrayList<Element>();

	public SubChapter(String name) {
		this.name = name;
	}

	public void createNewParagraph(String paragraph) {
		Paragraph para = new Paragraph(paragraph);
		this.e.add(para);
	}

	public void createNewImage(String image) {
		Image img = new Image(image);
		this.e.add(img);
	}

	public void createNewTable(String table) {
		Table titl = new Table(table);
		this.e.add(titl);
	}

	public String print() {

		System.out.println(this.name);

		Iterator<Paragraph> i_p = p.iterator();
		Iterator<Image> i_i = i.iterator();
		Iterator<Table> i_t = t.iterator();

		while (i_p.hasNext()) {

			System.out.println(i_p.next().print());
		}

		while (i_i.hasNext()) {

			System.out.println(i_i.next().print());
		}

		while (i_t.hasNext()) {

			System.out.println(i_t.next().print());
		}
		return false;
	}

}
